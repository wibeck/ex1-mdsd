  <eClassifiers xsi:type="ecore:EClass" name="AssemblyContext" eSuperTypes="#//Context componentModel.ecore#//AssemblyViewType">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="reqInterfacesFromRequiredRoles providedInterfacesFromProvidedRoles onlyDelegationConnectorsIfComposite providedRoleForProvidedInterfaceExistsAndIsConnected requiredRoleForRequiredInterfaceExistsAndIsConnected"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="reqInterfacesFromRequiredRoles" value="self.requiredInterfaces = self.requiredRoles -> collect(interFace)"/>
      <details key="providedInterfacesFromProvidedRoles" value="self.providedInterfaces = self.providedRoles -> collect(interFace)"/>
      <details key="onlyDelegationConnectorsIfComposite" value="if self.instantiates.oclIsKindOf(CompositeComponent) &#xA;&#x9;&#x9;then self.delegationConnectors -> size() > 0 else self.delegationConnectors -> size() = 0 endif"/>
      <details key="providedRoleForProvidedInterfaceExistsAndIsConnected" value="&#xA;&#x9;&#x9;&#x9;if self.instantiates.oclIsKindOf(CompositeComponent) then &#xA;&#x9;&#x9;&#x9;self.providedInterfaces -> forAll(interFace | self.instantiates.oclAsType(CompositeComponent).encapsulatedAssemblyContexts &#xA;&#x9;&#x9;&#x9;&#x9;-> collect(providedRoles) -> exists(role | role.interFace = interFace and self.delegationConnectors -> exists(connector | &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;connector.role = role and connector.interFace = interFace and connector.oclIsKindOf(assembly::ProvidedDelegationConnector)&#xA;&#x9;&#x9;&#x9;&#x9;))) else true endif"/>
      <details key="requiredRoleForRequiredInterfaceExistsAndIsConnected" value="if self.instantiates.oclIsKindOf(CompositeComponent) then &#xA;&#x9;&#x9;self.requiredInterfaces -> forAll(interFace | self.instantiates.oclAsType(CompositeComponent).encapsulatedAssemblyContexts &#xA;&#x9;&#x9;&#x9;&#x9;-> collect(requiredRoles) -> exists(role | role.interFace = interFace and self.delegationConnectors -> exists(connector | &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;connector.role = role and connector.interFace = interFace and connector.oclIsKindOf(assembly::RequiredDelegationConnector)&#xA;&#x9;&#x9;&#x9;&#x9;))) else true endif"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="instantiates" ordered="false"
        lowerBound="1" eType="ecore:EClass componentModel.ecore#//Component"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="requiredRoles" ordered="false"
        upperBound="-1" eType="#//RequiredRole" containment="true" eKeys="#//Role/name"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="providedRoles" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//ProvidedRole" containment="true"
        eKeys="#//Role/name"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownerComponent" ordered="false"
        eType="#//CompositeComponent"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AssemblyConnector">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="assemblyContextsNotEqual providedRoleHasRequiredInterface"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="assemblyContextsNotEqual" value="(not (self.providedRole.assemblyContext = self.requiredRole.assemblyContext)) = true"/>
      <details key="providedRoleHasRequiredInterface" value="self.providedRole.interFace = self.requiredRole.interFace"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="providedRole" lowerBound="1"
        eType="#//ProvidedRole"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="requiredRole" lowerBound="1"
        eType="#//RequiredRole"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RequiredDelegationConnector" eSuperTypes="#//DelegationConnector">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="roleCanOnlyBeRequired"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="roleCanOnlyBeRequired" value="self.role.oclIsKindOf(RequiredRole)"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RequiredRole" eSuperTypes="#//Role">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="requiredInterfaceFromServiceThatRequiresIt"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="requiredInterfaceFromServiceThatRequiresIt" value="self.assemblyContext.instantiates.interfaceServiceMap ->exists(entry | entry.services -> select(service| service.requiredInterfaces &#xA;&#x9;&#x9;-> includes(self.interFace)))  -> notEmpty() and not (self.assemblyContext.instantiates = null)"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ProvidedRole" eSuperTypes="#//Role">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="providededInterfaceFromISMap"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="providededInterfaceFromISMap" value="self.assemblyContext.instantiates.interfaceServiceMap ->exists(entry | entry.providedInterface = self.interFace)"/>
    </eAnnotations>
  </eClassifiers>
