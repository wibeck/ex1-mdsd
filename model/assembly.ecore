    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="System" eSuperTypes="#//Context">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="providedRoleForProvidedInterfaceExistsAndIsConnected requiredRoleForRequiredInterfaceExistsAndIsConnected"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="providedRoleForProvidedInterfaceExistsAndIsConnected" value="&#xA;&#x9;&#x9;self.providedInterfaces -> forAll(interFace | self.encapsulatedAssemblyContexts -> collect(providedRoles) &#xA;&#x9;&#x9;&#x9;-> exists(role | role.interFace = interFace and self.delegationConnectors -> exists(connector | connector.role = role &#xA;&#x9;&#x9;&#x9;&#x9;and connector.interFace = interFace and connector.oclIsKindOf(assembly::ProvidedDelegationConnector)) ))"/>
      <details key="requiredRoleForRequiredInterfaceExistsAndIsConnected" value="&#xA;&#x9;&#x9;self.requiredInterfaces -> forAll(interFace | self.encapsulatedAssemblyContexts -> collect(requiredRoles) &#xA;&#x9;&#x9;&#x9;-> exists(role | role.interFace = interFace and self.delegationConnectors -> exists(connector | connector.role = role &#xA;&#x9;&#x9;&#x9;&#x9;and connector.interFace = interFace and connector.oclIsKindOf(assembly::RequiredDelegationConnector)) ))"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="encapsulatedAssemblyContexts"
        ordered="false" upperBound="-1" eType="#//AssemblyContext" eKeys="componentModel.ecore#//ViewType/name"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
